<coref-experiment>
<system type="dummy:SoonDummyDecoder">
    <classifiers>
        <classifier type="maxent" model="idc0"
            options="** **"/>
    </classifiers>
    <extractors>
        <!-- general info about antecedent -->
        <extractor name="FE_MentionType_Buggy"/>
        <!-- agreement features -->
        <extractor name="FE_Gender"/>
        <extractor name="FE_Number"/>
        <!-- specialized features for aliases / appositive constructions -->
        <extractor name="FE_Alias"/>
        <extractor name="FE_Appositive"/>
        <!-- string matching features -->
        <extractor name="FE_StringMatch"/>
        <!-- semantic class agreement -->
        <extractor name="FE_SemanticClass"/>
        <extractor name="FE_DistDiscrete"/>
        <extractor name="FE_Appositive_iCab"/>
        <extractor name="FE_CoRef"/>
        <extractor name="FE_Yago"/>
        <extractor name="FE_Wiki"/>


    </extractors>
</system>
</coref-experiment>
