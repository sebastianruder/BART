<extractor name="FE_PrprName_StrMatch"/>
<extractor name="FE_Speech"/>
<extractor name="FE_StringKernel"/>
<extractor name="FE_SynPos"/>
<extractor name="FE_WebPatterns"/>
<extractor name="FE_Wiki"/>
<extractor name="FE_Yago"/>
 
<!-- recent additions -->
<extractor name="FE_Compatible"/>
