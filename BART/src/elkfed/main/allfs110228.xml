<coref-experiment>
<system type="soon">
    <classifiers>
        <classifier type="maxent" model="idc0"
            options="** **"/>
         <!-- classifier type="weka" model="idc0" learner="weka.classifiers.trees.J48"  options=""/ -->
    </classifiers>
    <extractors>
        <!-- general info about antecedent -->
        <extractor name="FE_MentionType_Coarse"/>
        <extractor name="FE_MentionType_Fine_Ana"/>
        <!-- extractor name="FE_MentionType_Fine_Ante"/ -->
        <!--agreement features -->
        <extractor name="FE_Gender"/>
        <extractor name="FE_Number"/>
        <!--specialized features for aliases / appositive constructions -->
        <extractor name="FE_Alias"/>
        <extractor name="FE_AppositiveParse"/>
        <!-- string matching features -->
        <extractor name="FE_StringMatch" />
        <!--semantic class agreement -->
        <extractor name="FE_AnimacyAgree"/>
        <extractor name="FE_SemClassAgree"/>
        <extractor name="FE_DistDiscrete"/>
        <!--random other stuff -->
        <extractor name="FE_SemClassValue"/>
        <extractor name="FE_BetterNames"/>
<extractor name="FE_Appositive_iCab"/>
        <extractor name="FE_First_Mention"/>
        <extractor name="FE_Coordination"/>



<!-- those are ok -->


<extractor name="FE_BothLocation"/>
<extractor name="FE_CorefChain"/>
