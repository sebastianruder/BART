<!-- extractor name="FE_DistanceWord"/ -->
<extractor name="FE_FirstSecondPerson"/>
<extractor name="FE_HeadMatch"/>
<extractor name="FE_HeadPartOfSpeech"/>
<extractor name="FE_LeftRightMatch"/>
<extractor name="FE_MinSame"/>

<extractor name="FE_NonProSalience"/>
